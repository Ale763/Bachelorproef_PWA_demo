>\n        <v-container grid-list-md>\n            <v-layout v-if=\"offline\" row wrap align-center red style=\"width: 109%;\n    margin: 0px;\n    position: relative;\n    left: -16px;\n    color: white;\n    top: -16px;\">\n                <v-flex xs2 style=\"text-align: center;\">\n                    <v-icon color=\"white\">cloud_off</v-icon>\n                </v-flex>\n                <v-flex xs10>\n                    <h2>No network connection. Functionality is limited.</h2>\n                </v-flex>\n            </v-layout>\n            <h1 >Category: {{ this.catId }}</h1>\n            <v-layout row\n                      fill-height\n                      wrap\n            >\n            <div v-if=\"!    loaded\">Loading....</div>\n                    <Product v-for=\"(product, index) in products\"\n                             v-if=\"product\"\n                             :sku=\"product.getSku()\"\n                             :key=\"product.getSku()\"\n                             style=\"display: inline-block;\" />\n            </v-layout>\n\n\n        </v-container>\n    </div>\n</template>\n\n<script>\n\timport Product from \"../components/Product\";\n\n\timport {MagentoAPIService} from \"../services/MagentoAPIService\";\n\n\texport default {\n\t\tname: \"Category\",\n\t\tcomponents: {\n\t\t\tProduct,\n\t\t},\n\t\tprops:\n\t\t{},\n\t\tdata()\n\t\t{\n\t\t\treturn {\n\t\t\t\tcatId: '',\n\t\t\t\tcategorIndex:\n\t\t\t\t{\n\t\t\t\t\t'Women-Tops':\n\t\t\t\t\t{\n\t\t\t\t\t\t'products': ['WT09', 'WT08', 'WT07', 'WT06', 'WT05', 'WT04']\n\n\t\t\t\t\t},\n\t\t\t\t\t'Women-Bottoms':\n\t\t\t\t\t{\n\t\t\t\t\t\t'products': ['WSH12-28-Green', 'WSH11-28-Red', 'WSH10-28-Black', 'WSH09-28-Green',\n                            'WSH08-28-Purple', 'WSH07-28-Black']\n\t\t\t\t\t},\n\t\t\t\t\t'Men-Tops':\n\t\t\t\t\t{\n\t\t\t\t\t\t'products': ['MT12', 'MT11', 'MT10']\n\t\t\t\t\t},\n\t\t\t\t\t'Men-Bottoms':\n\t\t\t\t\t{\n\t\t\t\t\t\t'products': ['MSH12', 'MSH11', 'MSH10']\n\t\t\t\t\t},\n\t\t\t\t\t'Men-Accessories':\n\t\t\t\t\t{\n\t\t\t\t\t\t'products': ['MA01', 'MA02']\n\t\t\t\t\t},\n\t\t\t\t},\n                offline: false,\n                loaded: false\n\t\t\t}\n\t\t},\n\t\tcomputed:\n\t\t{\n\t\t\tposts: function ()\n\t\t\t{\n\t\t\t\tlet posts = this.$store.getters['posts/allPosts'];\n\t\t\t\t// console.log(\"All Posts: \", posts);\n\t\t\t\treturn posts;\n\t\t\t},\n\t\t\tproducts: function ()\n\t\t\t{\n\t\t\t\t// let products = this.$store.getters['products/allProducts'];\n\t\t\t\t// let filteredProducts = [];\n\t\t\t\t// for (let key of this.categorIndex[this.catId]['products'])\n\t\t\t\t// {\n\t\t\t\t// \tlet product = this.$store.dispatch('products/fetchProduct', key)\n\t\t\t\t// \t                  .then((data) =>\n\t\t\t\t// \t                        {\n\t\t\t\t// \t\t                        filteredProducts.push(data);\n                //\n\t\t\t\t// \t                        })\n\t\t\t\t// }\n\t\t\t\t// console.log(\"All Posts: \", posts);\n\t\t\t\t// for (let i = 0; i < filteredProducts.length; ++ i)\n\t\t\t\t// {\n\t\t\t\t// \tfor (let j = 0; j < filteredProducts.length; ++ j)\n\t\t\t\t// \t{\n\t\t\t\t// \t\tif (filteredProducts[i].getSku() > filteredProducts[j].getSku())\n\t\t\t\t// \t\t{\n\t\t\t\t// \t\t\tlet temp = filteredProducts[i];\n\t\t\t\t// \t\t\tfilteredProducts[i] = filteredProducts[j];\n\t\t\t\t// \t\t\tfilteredProducts[j] = temp;\n\t\t\t\t// \t\t}\n\t\t\t\t// \t}\n\t\t\t\t// }\n                let index  = this.categorIndex[this.catId]['products'];\n\t\t\t\tlet filteredProducts = this.$store.getters['products/filterProducts'](index);\n\t\t\t\tif (filteredProducts == null)\n                {\n                \treturn [];\n\t                // filteredProducts = this.loadCategoryProducts();\n\t                // filteredProducts = this.$store.getters['products/filterProducts'](index);\n                }\n\t\t\t\tthis.loaded = true;\n\t\t\t\treturn filteredProducts;\n\t\t\t}\n\t\t},\n\t\tcreated()\n\t\t{\n\t\t\tthis.setPageCategory(this.$route.params.catName);\n\t\t\tthis.loadCategoryProducts();\n\t\t\t// this.fetchProduct(\"24-MB01\");\n\t\t\t// this.fetchProduct(\"WS12\");\n\t\t},\n\t\tmethods:\n\t\t{\n\t\t\tgetPageCategory()\n\t\t\t{\n\t\t\t},\n\t\t\tsetPageCategory(category)\n\t\t\t{\n\t\t\t\tthis.catId = category;\n\t\t\t},\n\t\t\tfetchProduct(sku)\n\t\t\t{\n\t\t\t\treturn this.$store.dispatch('products/fetchProduct', sku)\n\t\t\t\t    .then((data) =>\n\t\t\t\t          {\n\t\t\t\t\t          // console.log('Fetch ok', data);\n                              return data;\n\t\t\t\t          })\n\t\t\t\t    .catch((error) =>\n\t\t\t\t           {\n\t\t\t\t\t\t\t   if (!navigator.onLine)\n\t\t\t\t\t\t\t   {\n\t\t\t\t\t\t\t\t   console.log(\"Offline now\");\n\t\t\t\t\t\t\t   }\n\t\t\t\t\t           else if (error.message === \"Offline\")\n\t\t\t\t\t           {\n\t\t\t\t\t\t           console.log(\"Network error: \", error)\n\t\t\t\t\t           } else\n\t\t\t\t\t\t           console.error('Fetch error', error);\n\t\t\t\t           });\n\t\t\t},\n\t\t\tloadCategoryProducts()\n\t\t\t{\n\t\t\t\tlet result = [];\n\t\t\t\tconst categoryProducts = this.categorIndex[this.catId]['products'];\n\t\t\t\tfor (let productSKU of categoryProducts)\n\t\t\t\t{\n\t\t\t\t\tresult.push(this.fetchProduct(productSKU));\n\t\t\t\t}\n\t\t\t\treturn result;\n\t\t\t}\n\n\t\t},\n\t\twatch: {\n\t\t\t$route(to, from)\n\t\t\t{\n\t\t\t\tthis.setPageCategory(to.path.split('/category/')[1]);\n\t\t\t\tthis.loadCategoryProducts();\n\t\t\t\tconsole.log(\"Category route change\");\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped>\n    .category {\n        /*max-width: 1000px;*/\n        margin: 0 auto;\n    }\n\n    @media only screen and (max-width: 960px) {\n        .my-offset-xs1 {\n            margin: 0 8.333333333333%;\n        }\n\n    }\n</style>\n"]}]}